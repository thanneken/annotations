		filios dicens ad eos<lb/>
		rogans <milestone unit="vs" n="2"/>uidete filii<lb/>
		ecce ultio facta est<lb/>
		in plebe altera cru<lb type="inWord"/>
		delis inmunda et<lb/>
		traductio sine mi<lb type="inWord"/>
		sericordia et emi<lb type="inWord"/>
		nent principatum<lb/>
		<milestone unit="vs" n="3"/>quae enim gens aut<lb/>
